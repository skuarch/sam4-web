<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>connection</servlet-name>
        <servlet-class>net.connection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>linker</servlet-name>
        <servlet-class>net.linker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>linkerFile</servlet-name>
        <servlet-class>net.linkerFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>objectRequester</servlet-name>
        <servlet-class>net.objectRequester</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ObjectDistributor</servlet-name>
        <servlet-class>net.ObjectDistributor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ObjectDispenser</servlet-name>
        <servlet-class>net.ObjectDispenser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>blocksite</servlet-name>
        <servlet-class>blocksite</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>connection</servlet-name>
        <url-pattern>/connection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>linker</servlet-name>
        <url-pattern>/linker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>linkerFile</servlet-name>
        <url-pattern>/linkerFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>objectRequester</servlet-name>
        <url-pattern>/objectRequester</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObjectDistributor</servlet-name>
        <url-pattern>/ObjectDistributor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObjectDispenser</servlet-name>
        <url-pattern>/ObjectDispenser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>blocksite</servlet-name>
        <url-pattern>/blocksite</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
